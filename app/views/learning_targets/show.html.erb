<h1>
  <%= @lt.name %> for class <%= link_to(@klass.name, klass_path(@klass)) %>
</h1>
<p>
  <%= link_to("Edit Learning Target", edit_klass_learning_target_path(@klass, @lt)) %>
</p>


<div class="lt-show-container">
  <div class="lt-show-gradebook">
    <%= render partial: "klasses/table", locals: {klass: @klass, collection: [@lt], klass_or_lt: @lt} %>
  </div>

  <div class="lt-show-rubric">
    <%= render partial: "learning_targets/rubric", locals: {klass: @klass, lt: @lt} %>
  </div>

    <div>
      <%= line_chart(@lt.graph_data, width: "400px", height: "400px", xtitle: "Date", ytitle: "Avg Score", messages: {empty: "No data"}, colors: ["#b00", "#666"], max: 4) %>
    </div>

</div>
