<%= content_tag(:tr) do %>
  <%= td_strong_label("Students") %>
  <%= td_strong_label("Average") %>
  <% collection.each do |lt| %>
    <%= blank_td?(lt) %>
    <% lt.chronological_assignments.each_with_index do |assignment, index| %>
      <%= td_display_average(klass, assignment, index) %>
    <% end %>
  <% end %>
<% end %>
