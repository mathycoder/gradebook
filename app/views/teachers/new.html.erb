<h1>Create a new Teacher account</h1>

<%= form_for(@teacher) do |f| %>

  <div>
    <%= f.label(:name, "Enter your name") %><br>
    <%= f.text_field(:name) %><br><br>
  </div>

  <div>
    <%= f.label(:email, "Enter your email") %><br>
    <%= f.text_field(:email) %><br><br>
  </div>


  <%= f.submit %>

<% end %>
