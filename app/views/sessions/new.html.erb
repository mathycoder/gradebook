<div class="paper-form">
  <h1>Log in Page</h1>
  <%= form_for(@teacher, :url => {controller: 'sessions', action: 'create', method: 'post'}) do |f| %>

    <div>
      <%= f.label(:email, "Enter your email") %><br>
      <%= f.text_field(:email) %><br><br>
    </div>

    <%= f.submit("Log in") %>
  <% end %>
</div>
